<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity ProductDefinition.
    -->
    <changeSet id="20200803145334-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="term_id"
                                 baseTableName="product_definition"
                                 constraintName="fk_product_definition_term_id"
                                 referencedColumnNames="id"
                                 referencedTableName="term"/>

        <addForeignKeyConstraint baseColumnNames="currency_id"
                                 baseTableName="product_definition"
                                 constraintName="fk_product_definition_currency_id"
                                 referencedColumnNames="id"
                                 referencedTableName="currency"/>

    </changeSet>
</databaseChangeLog>
